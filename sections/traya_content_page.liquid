<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css">
<section class="bg-light">
  <div class="container pt-5">
    <di class="row">
      <div class="col-md-12">
        <div class="heading">
          <h3>{{ section.settings.heading }}</h3>
        </div>
        <div class="paragraph1">
          <p>{{ section.settings.para1 }}</p>
        </div>
      </div>
    </di>
  </div>
</section>
{% schema %}
  {
    "name": "content-page",
    "settings":[
    {
      "type": "text",
      "id": "para1",
      "label": "Heading",
      "default": "Quick links"
    },
    {
      "type": "text",
      "id": "heading",
      "label": "Heading",
      "default": "Quick links"
    }     
  ],
  "presets":[
  
              {
                "name":"content-page",
                "category":"custom"
              }
            ]
}
{% endschema %}

<style>
  .heading h3{
    font-size: 12pt;
    font-family: Arial;
    color: #222222;
    font-weight: 700;
    text-decoration: underline;
  }
  .paragraph1 p{
    line-height: 20px;
    font-size:14px;
    margin-top: 8.8px;
}
  }
</style>